@{
    Layout = null;
}

<html>
<head>
    <title></title>
    <script src="~/ui/jq/jquery-1.10.1.min.js"></script>
</head>
<body>
    <form class="fm" action="@(ViewData["callback"])" method="post" style="display:none;">
        <input type="hidden" name="url" value="@(ViewData["url"])" />
        <input type="hidden" name="email" value="@(ViewData["email"])" />
        <input type="hidden" name="token" value="@(ViewData["token"])" />
    </form>
    <script type="text/javascript">
        $(function () {
            $('.fm').submit();
        });
    </script>
</body>
</html>
